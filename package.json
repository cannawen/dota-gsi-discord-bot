        "@types/jest": "^29.4.0",
        "@types/node": "^18.15.0",
        "@types/node-cron": "^3.0.7",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "eslint": "^8.38.0",
        "eslint-plugin-no-loops": "^0.3.0",
        "nodemon": "^2.0.21",
        "rimraf": "^4.4.0",
        "supertest": "^6.3.3",
        "ts-essentials": "^9.3.1",
        "ts-jest": "^29.0.5",
