{
    "name": "dota-discord-bot",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean && tsc",
        "start": "node build/index.js",
        "go": "npm run build && npm run start",
        "clean": "rimraf ./build ./resources/audio/tts-cache ./data ./test_PERSISTENCE_DATA_PATH",
        "test": "jest --detectOpenHandles",
        "coverage": "npm test -- --coverage --collectCoverageFrom=\"./src/**\"",
        "discord": "npm run build && node build/discord/deployCommands.js",
        "lint": "eslint . --ext .ts",
        "format": "npx prettier --write .",
        "start:dev": "npx nodemon",
        "test:dev": "jest --watchAll",
        "rosh-canna": "mitmdump -nC mitmproxy-flows/kill-rosh-take-bounty-destroy-tower-live_canna",
        "rosh-canna-remote": "mitmdump -M \"|~all|http://localhost:9001/gsi|http://dota-coach.fly.dev/gsi\" -nC mitmproxy-flows/kill-rosh-take-bounty-destroy-tower-live_canna",
        "dig-canna": "mitmdump -nC mitmproxy-flows/die-respawn-dig_canna",
        "dig-canna-remote": "mitmdump -M \"|~all|http://localhost:9001/gsi|http://dota-coach.fly.dev/gsi\" -nC mitmproxy-flows/die-respawn-dig_canna",
        "rune-canna": "mitmdump -nC mitmproxy-flows/water-rune_canna",
        "rune-canna-remote": "mitmdump -M \"|~all|http://localhost:9001/gsi|http://dota-coach.fly.dev/gsi\" -nC mitmproxy-flows/water-rune_canna",
        "rune-wrath": "mitmdump -nC mitmproxy-flows/water-rune_wrath",
        "rune-wrath-remote": "mitmdump -M \"|~all|http://localhost:9001/gsi|http://dota-coach.fly.dev/gsi\" -nC mitmproxy-flows/water-rune_wrath"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@colors/colors": "^1.5.0",
        "@discordjs/voice": "^0.15.0",
        "axios": "^1.3.4",
        "crypto-js": "^4.1.1",
        "deep-equal": "^2.2.0",
        "discord-speech-to-text": "github:cannawen/discord-stt#master",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.3",
        "eslint-plugin-jest": "^27.2.1",
        "express": "^4.18.2",
        "ffmpeg-static": "^5.1.0",
        "libsodium-wrappers": "^0.7.11",
        "node-cron": "^3.0.2",
        "node-gsi": "github:cannawen/node-gsi#master",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
